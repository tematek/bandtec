<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <h1>Chorão Syckness</h1>
   
    <div id="body-menus">

        @Html.ActionLink("Home", "Index", "Home")
        @Html.ActionLink("Sobre", "Sobre", "Home")
        @Html.ActionLink("Contato", "Contato", "Home")
        @Html.ActionLink("Galeria", "Galeria", "Galeria")
        @Html.ActionLink("Cadastro", "Usuario", "Cadastro")
        @Html.ActionLink("Login", "LoginUsuario", "Login")

    </div>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)

    <footer>
        <p>choraosyckness.com.br - todos os direitos reservados.</p>
    </footer>

</body>

